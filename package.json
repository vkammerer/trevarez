{
  "name": "trevarez",
  "version": "0.0.5",
  "description": "Borne tactile pour l'exposition permanente \"Bâtir un rêve\" au château de Trévarez.",
  "scripts": {
    "clean": "rimraf release",
    "clean:osx": "rimraf release/osx",
    "clean:win": "rimraf release/win",
    "clean:linux": "rimraf release/linux",
    "start": "electron static",
    "app:osx": "electron-packager static Trevarez --platform=darwin --arch=x64 --out=release/osx --version=0.35.0 --overwrite",
    "app:win": "electron-packager static Trevarez --platform=win32 --arch=x64 --out=release/win --version=0.35.0 --overwrite",
    "app:linux": "electron-packager static Trevarez --platform=linux --arch=x64 --out=release/linux --version=0.35.0 --overwrite",
    "installer:osx": "electron-builder release/osx/Trevarez-darwin-x64/Trevarez.app --platform=osx --out=release/osx --config=resources/config.json",
    "installer:win": "electron-builder release/win/Trevarez-win32-x64 --platform=win --out=release/win --config=resources/config.json",
    "dev": "node server-dev.es5.js",
    "build": "webpack --colors --progress --config webpack.config.prod.es5.js",
    "build:osx": "npm run clean:osx && npm run app:osx && npm run installer:osx",
    "build:win": "npm run clean:win && npm run app:win && npm run installer:win",
    "deploy": "git subtree push --prefix static origin gh-pages",
    "test": "mocha test/**/*.test.js* -R spec --compilers css:./test/misc/null-compiler,md:./test/misc/null-compiler,jsx:babel-core/register --recursive",
    "coverage": "istanbul cover --dir ./reports _mocha -- -R spec test/**/*.test.js* --compilers css:./test/misc/null-compiler,md:./test/misc/null-compiler,jsx:babel-core/register --recursive",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=b02ea30e27e20150c80e3e084f8b02bea77bb7fa0cbeaf8e73dbccd54a31085b codeclimate-test-reporter < reports/lcov.info",
    "ci": "npm run clean && npm run coverage && npm run codeclimate && npm run build:win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xseignard/trevarez.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/xseignard/trevarez#readme",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "chai": "^3.4.1",
    "classnames": "^2.2.0",
    "codeclimate-test-reporter": "^0.1.1",
    "css-loader": "^0.23.0",
    "electron-builder": "^2.2.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.35.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "istanbul": "^0.4.1",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.3.4",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "teleporter": "0.0.5",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
