{
  "name": "trevarez",
  "version": "0.0.1",
  "description": "Borne tactile pour l'exposition permanente \"Bâtir un rêve\" au château de Trévarez.",
  "scripts": {
    "clean:osx": "rimraf release/osx",
    "clean:win": "rimraf release/win",
    "start": "electron static",
    "release:osx": "electron-packager static Trevarez --platform=darwin --arch=x64 --out=\"release/osx\" --version=0.35.0 --overwrite",
    "release:win": "electron-packager static Trevarez --platform=win32 --arch=x64 --out=release/win --version=0.35.0 --overwrite",
    "release:linux": "electron-packager static Trevarez --platform=linux --arch=x64 --out=release/linux --version=0.35.0 --overwrite",
    "installer:osx": "electron-builder \"release/osx/Trevarez-darwin-x64/Trevarez.app\" --platform=osx --out=\"release/osx\" --config=resources/config.json",
    "installer:win": "electron-builder release/Trevarez-win32-x64 --platform=win --out=release/win --config=resources/config.json",
    "dev": "node server-dev.es5.js",
    "build": "webpack --config webpack.config.prod.es5.js",
    "build:osx" : "npm run clean:osx && npm run release:osx && npm run installer:osx",
    "build:win" : "npm run clean:win && npm run release:win && npm run installer:win",
    "deploy": "git subtree push --prefix static origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkammerer/trevarez.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/vkammerer/trevarez#readme",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "classnames": "^2.2.0",
    "css-loader": "^0.23.0",
    "electron-builder": "^2.2.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.35.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
